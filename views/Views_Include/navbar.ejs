<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--========== BOX ICONS ==========-->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css"
    />
    <script type="text/javascript" src="Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="Scripts/jquery-2.1.1.min.js"></script>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <!--========== CSS ==========-->
    <link href='http://localhost:3000/css/nav.css' rel='stylesheet' type='text/css'>
    <title>Responsive sidebar submenus</title>
    <style>
    </style>
  </head>
  <body>
    <!--========== HEADER ==========-->

    <header class="header">
      <div class="header__container">
        <!-- <img src="./Logo.png" alt="" class="header__img" /> -->
        <div class="header__img">
          <div
            class="btn-group"
            role="group"
            aria-label="Button group with nested dropdown"
          >
            <button type="button" class="btn btn-secondary bg-danger">
              <i class="fa-sharp fa-solid fa-phone"></i>
            </button>
            <button type="button" class="btn btn-secondary">
              <i class="fa-sharp fa-solid fa-envelope"></i>
            </button>

            <div class="btn-group" role="group">
              <button
                id="btnGroupDrop1"
                type="button"
                class="btn btn-secondary dropdown-toggle"
                data-toggle="dropdown"
                aria-haspopup="true"
                aria-expanded="false"
              >
                Truy cập
              </button>

              <div
                class="dropdown-menu bg-info"
                aria-labelledby="btnGroupDrop1"
              >
                <a
                  class="dropdown-item"
                  style="
                    font-weight: 700;
                    font-family: Arial, Helvetica, sans-serif;
                  "
                  href="/views_nguoidung_dangnhap"
                  >Đăng nhập</a
                >
                <a
                  class="dropdown-item"
                  style="
                    font-weight: 700;
                    font-family: Arial, Helvetica, sans-serif;
                  "
                  href="/views_nguoidung_dangky"
                  >Đăng ký</a
                >
              </div>
            </div>
          </div>
        </div>

        <a href="/" class="header__logo"
          >Luagasinhvien.vn <i class="fa-sharp fa-solid fa-house-circle-check"></i
        ></a>

        <div class="header__search">
          <input
            type="search"
            placeholder="Bạn muốn tìm gì?"
            class="header__input"
          />
          <i class="bx bx-search header__icon"></i>
        </div>

        <div class="header__toggle">
          <i class="bx bx-menu" id="header-toggle"></i>
        </div>
      </div>
    </header>

    <!--========== NAV ==========-->
    <div class="nav" id="navbar">
      <nav class="nav__container">
        <div>
          <a href="/admin/them" class="nav__link nav__logo">
            <i class="bx bxs-disc nav__icon"></i>
            <span class="nav__logo-name">Admin login</span>
          </a>

          <div class="nav__list">
            <div class="nav__items">
              <h3 class="nav__subtitle">Người dùng</h3>

              <a href="#" class="nav__link active">
                <i class="bx bx-home nav__icon"></i>
                <span class="nav__name">Đăng tin+</span>
              </a>

              <div class="nav__dropdown">
                <a href="#" class="nav__link">
                  <i class="bx bx-user nav__icon"></i>
                  <span class="nav__name">Quản lý</span>
                  <i
                    class="bx bx-chevron-down nav__icon nav__dropdown-icon"
                  ></i>
                </a>

                <div class="nav__dropdown-collapse">
                  <div class="nav__dropdown-content">
                    <a href="#" class="nav__dropdown-item">Bài đăng</a>
                    <a href="#" class="nav__dropdown-item">Nhà trọ</a>
                    <a href="#" class="nav__dropdown-item">Phòng trọ</a>
                    <a href="#" class="nav__dropdown-item">Người thuê</a>
                    <a href="#" class="nav__dropdown-item">Danh sách thuê</a>
                    <a href="#" class="nav__dropdown-item">Đánh giá</a>
                   
                  </div>
                </div>
              </div>

              <a href="" class="nav__link">
                <i class="bx bx-message-rounded nav__icon"></i>
                <span class="nav__name">Hồ sơ!</span>
              </a>
            </div>

            <div class="nav__items">
              <h3 class="nav__subtitle">Quản trị viên</h3>

              <div class="nav__dropdown">
                <a href="" class="nav__link">
                  <i class="bx bx-store nav__icon"></i>
                  <span class="nav__name">Quản lý</span>
                  <i
                    class="bx bx-chevron-down nav__icon nav__dropdown-icon"
                  ></i>
                </a>

                <div class="nav__dropdown-collapse">
                  <div class="nav__dropdown-content">
                    <a href="/views_danhsach_admin" class="nav__dropdown-item"
                      >Quản trị viên</a
                    >
                    <a
                      href="/views_danhsach_nguoidung"
                      class="nav__dropdown-item"
                      >Người dùng</a
                    >
                    <a href="/nhatro/danhsach" class="nav__dropdown-item">Nhà trọ</a>
                    <a href="#" class="nav__dropdown-item">Phòng trọ</a>
                    <a href="/views_danhsach_baidang" class="nav__dropdown-item"
                      >Bài đăng</a
                    >
                    <a href="/views_danhsach_tienich" class="nav__dropdown-item">Tiện ích</a>
                  </div>
                </div>
              </div>

              <a href="#" class="nav__link">
                <i class="bx bx-compass nav__icon"></i>
                <span class="nav__name">Cài đặt</span>
              </a>
              <a href="#" class="nav__link">
                <i class="bx bx-bookmark nav__icon"></i>
                <span class="nav__name">Xem thêm</span>
              </a>
            </div>
          </div>
        </div>

        <a href="/dangxuat" class="nav__link nav__logout">
          <i class="bx bx-log-out nav__icon"></i>
          <span class="nav__name">Đăng xuất</span>
        </a>
      </nav>
    </div>

    <!--========== CONTENTS ==========-->

    <!--========== MAIN JS ==========-->
    <script src="main.js"></script>
  </body>
</html>
